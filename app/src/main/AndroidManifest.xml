        <activity android:name=".view.Implement.RegisterUsuarioActivity" />
        <activity android:name=".view.Implement.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>